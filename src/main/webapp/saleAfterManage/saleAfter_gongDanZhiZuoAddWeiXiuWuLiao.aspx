                $("#ddlSuoShuZhangTao option").each(function () {
                    if ($(this).val() == "正常维修") {
                        $(this).remove();
                    }
                });
            } else {
                //非保险
                $("#ddlSuoShuZhangTao option").each(function () {
                    if ($(this).val() == "保险" || $(this).val() == "保险自付") {
                        $(this).remove();
                    }
                });

            }
            //帐套判断 end

        });


